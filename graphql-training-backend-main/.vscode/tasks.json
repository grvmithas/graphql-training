{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start Development Server",
      "type": "shell",
      "command": "npm run dev",
      "problemMatcher": ["$tsc-watch"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": true
    },
    {
      "label": "Build Project",
      "type": "shell",
      "command": "npm run build",
      "problemMatcher": ["$tsc"],
      "group": {
        "kind": "build"
      }
    }
  ]
}
